{
  "name": "portfolio-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portfolio-web",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project portfolio-web --web",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}",
        "{options.outputPath}/.next/**",
        "!{options.outputPath}/.next/cache/**"
      ],
      "options": {
        "outputPath": "dist/portfolio-web"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "portfolio-web:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "portfolio-web:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "portfolio-web:build:production",
          "dev": false
        }
      }
    }
  }
}
